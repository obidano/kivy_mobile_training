<ListeEtudiantsScreen>:
    MDFloatLayout:
        md_bg_color: utils.get_color_from_hex("#ffffff")
        MDTopAppBar:
            pos_hint: {"top":1}
            MDTopAppBarLeadingButtonContainer:
                MDActionTopAppBarButton:
                    icon: "arrow-left-thin"
            MDTopAppBarTitle:
                text: "Etudiants"
            MDTopAppBarTrailingButtonContainer:
                MDActionTopAppBarButton:
                    icon: "calendar"
                MDActionTopAppBarButton:
                    icon: "account-circle-outline"
        MDBoxLayout:
            pos_hint: {"top": 0.9}
            orientation: "vertical"
            padding: dp(20)
            MDLabel:
                text: "Etudiants (16)"
                id: label_id
            ScrollView:
                MDList:
                    id: ma_liste_de_donnee
            Widget:
